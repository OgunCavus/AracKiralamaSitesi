@model IEnumerable<AracKiralamaDB.Models.Kullanicilar>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Kullanıcılar</h2>

<p>
   
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.KullaniciAdi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Sifre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Kullanıcı_Tipi)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.KullaniciAdi)
        </td>
        <td>
            @Html.ValidationMessageFor(modelItem => item.Sifre, "****", new { @class = "error" })
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Kullanıcı_Tipi)
        </td>
       
    </tr>
}

</table>
